<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.wm.cs.cs301.isabellawu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wm_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wm_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AMazeByIsabellaWu">
        <activity
            android:name=".gui.LosingActivity"
            android:exported="true" />
        <activity
            android:name=".gui.WinningActivity"
            android:exported="true" />
        <activity
            android:name=".gui.PlayAnimationActivity"
            android:exported="true" />
        <activity
            android:name=".gui.PlayManuallyActivity"
            android:exported="true" />
        <activity
            android:name=".gui.GeneratingActivity"
            android:exported="true" />
        <activity
            android:name=".gui.AMazeActivity"
            android:exported="true"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>